{
  "name": "@proccheck-html/core",
  "version": "0.0.0",
  "description": "Core functions of the proccheck-html ",
  "keywords": [
    "performance",
    "website"
  ],
  "author": "jadok <jadok.6me@mail.com>",
  "homepage": "https://github.com/proccheck/proccheck-html/tree/master/packages/core#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "cross-env ts-node ./src/index.ts",
    "build": "microbundle bundle",
    "dev": "microbundle watch",
    "prettier": "cross-env ./node_modules/.bin/prettier '**/*.{json,md,scss,yaml,yml}'",
    "tslint:fix": "cross-env ./node_modules/.bin/tslint --project tsconfig.json -e .gitignore 'src/**/*.{js,ts} --fix'",
    "test": "jest",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/jest --coverage",
    "test:coverall": "npm run test:coverage && cat ./coverage/lcov.info | cross-env ./node_modules/.bin/coveralls"
  },
  "bugs": {
    "url": "https://github.com/proccheck/proccheck-html/issues"
  },
  "dependencies": {
    "@types/node": "^11.11.6"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proccheck/proccheck-html.git"
  }
}
